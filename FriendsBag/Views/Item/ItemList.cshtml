
@{
    ViewBag.Title = "ItemList";
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}

<h2>ItemList</h2>

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Content/Pages/Item.js"></script>
<body>
    <div class="container">
        <div class="col-md-12">
            <div class="col-md-6" style="padding-left:68.5px">
                <table id="tblItem" class="table table-striped table-bordered" border="1">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>ItemName</th>
                            <th>ItemPrice</th>
                            <th>Discount</th>
                            <th>Gender</th>
                            <th>Colour</th>
                            <th>Description</th>
                            <th>Date</th>
                            <th>Image</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</body>